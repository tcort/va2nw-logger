        <section id="logs" class="navtarget">
            <h3>Search</h3>
            <form name="search" method="GET" action="/logs">
                Search by callsign:
                <input type="text" name="callsign" placeholder="callsign" />
                <input type="hidden" name="since" value="1900-01-01T00:00:00Z" />
                <input type="submit" value="Search" />
            </form>

            <h3><a name="logbook"></a>Logbook</h3>
            <table>
                <thead>
                    <th>&nbsp;</th>
                    <th>Date</th>
                    <th>Time</th>
                    <th>Callsign</th>
                    <th>RST Sent</th>
                    <th>RST Recv</th>
                    <th>Freq</th>
                    <th>Pwr</th>
                    <th>Mode</th>
                    <th>Notes</th>
                </thead>
                <tbody>
                    {{#each logs}}
                        <tr id="{{LOG_ID}}" class="summary">
                            <td><button class="toggle-visibility" data-target="{{LOG_ID}}_details">+</button></td>
                            <td>{{DATE_HUMAN}}</td>
                            <td>{{TIME_HUMAN}}</td>
                            <td><a href="/logs?callsign={{CALL}}&since=1900-01-01T00:00:00Z#logbook">{{CALL}}</a></td>
                            <td>{{RST_SENT}}</td>
                            <td>{{RST_RCVD}}</td>
                            <td>{{FREQ}}</td>
                            <td>{{TX_PWR}}</td>
                            <td>{{MODE}}</td>
                            <td>{{NOTES}}</td>
                        </tr>
                        <tr id="{{LOG_ID}}_details" class="hidden">
                            <td colspan="10">
                                <p>
                                    {{MY_NAME}} ({{OPERATOR}}) operating station {{STATION_CALLSIGN}} running a {{MY_RIG}} transceiver at
                                    {{TX_PWR}} watts into a {{MY_ANTENNA}} antenna from
                                    {{#if MY_POTA_REF}}
                                        POTA Ref <a href="https://pota.app/#/park/{{MY_POTA_REF}}">{{MY_POTA_REF}}</a> in
                                    {{/if}}
                                    {{MY_STATE}}, {{MY_COUNTRY}} (Grid: {{MY_GRIDSQUARE}}, ITU Zone: {{MY_ITU_ZONE}}, CQ Zone: {{MY_CQ_ZONE}})
                                    made a {{MODE}} contact with <a href="/logs?callsign={{CALL}}#logbook">{{CALL}}</a>
                                    {{#if SKCC}}
                                        (SKCC #<a href="https://www.skccgroup.com/membership_data/membership_search.php?search={{SKCC}}">{{SKCC}}</a>)
                                    {{/if}}
                                    on {{FREQ}} MHz in the
                                    {{BAND}} band on {{DATE_HUMAN}} at {{TIME_HUMAN}} UTC with a signal report of {{RST_SENT}}/{{RST_RCVD}}
                                    {{#if POTA_REF}}
                                        in POTA Ref <a href="https://pota.app/#/park/{{POTA_REF}}">{{POTA_REF}}</a>
                                    {{/if}}
                                </p>
                                <p>
                                    Callsign Lookup:
                                        <a href="https://www.qrz.com/db/{{CALL}}">QRZ</a>,
                                        <a href="https://pota.app/#/profile/{{CALL}}">POTA</a>,
                                        <a href="https://www.skccgroup.com/membership_data/membership_search.php?search={{CALL}}">SKCC</a>,
                                        <a href="https://www.youtube.com/results?search_query={{CALL}}">YouTube</a>
                                </p>
                            </td>
                        </tr>
                    {{/each}}
                </tbody>
            </table>
        </section>

